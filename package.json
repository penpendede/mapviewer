{
  "name": "mapviewer",
  "version": "0.0.1",
  "description": "A simple mapviewer",
  "main": "renderer.js",
  "scripts": {
    "precommit": "npm test",
    "start": "electron ./",
    "test": "npm run test-html && npm run test-js && npm run test-json",
    "test-html": "eslint  --ext .html .",
    "test-js": "standard",
    "test-json": "jsonlint -q config/*"
  },
  "author": "Josef Schugt",
  "repository": {
    "type": "git",
    "url": "https://github.com/penpendede/mapviewer.git"
  },
  "license": "MIT",
  "standard": {
    "ignore": [
      "leaflet"
    ]
  },
  "devDependencies": {
    "electron": "^1.8.1",
    "eslint": "^4.17.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-markdown": "^1.0.0-beta.7",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jsonlint": "^1.6.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "standard-json": "^1.0.2",
    "standard-reporter": "^1.0.5",
    "standard-summary": "^1.0.0"
  },
  "dependencies": {
    "leaflet": "^1.3.1"
  }
}
